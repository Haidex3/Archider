{
  "workbench.colorTheme": "light-rose-pine",

  "editor.semanticHighlighting.enabled": true,

  "workbench.colorCustomizations": {
    "breadcrumb.foreground": "#6E5E82",
    "editor.background": "#F8E6EE",
    "editor.foreground": "#0E0C12",
    "editorCursor.foreground": "#C06A78",

    "editor.selectionBackground": "#E6A3B4",
    "editor.inactiveSelectionBackground": "#F2D2DF",
    "editor.lineHighlightBackground": "#F2D2DF",

    "editor.wordHighlightBackground": "#E6A3B4",
    "editor.wordHighlightStrongBackground": "#C06A78",

    "editorLineNumber.foreground": "#6E5E82",
    "editorLineNumber.activeForeground": "#C06A78",

    "activityBar.background": "#F8E6EE",
    "activityBar.foreground": "#0E0C12",
    "activityBarBadge.background": "#C06A78",
    "activityBarBadge.foreground": "#FFFFFF",

    "sideBar.background": "#F8E6EE",
    "sideBar.foreground": "#1E1A26",
    "sideBarSectionHeader.background": "#FFEAF1",
    "sideBarSectionHeader.foreground": "#C06A78",

    "statusBar.background": "#F8E6EE",
    "statusBar.foreground": "#1E1A26",
    "statusBar.noFolderBackground": "#F8E6EE",
    "statusBar.debuggingBackground": "#C06A78",

    "titleBar.activeBackground": "#F8E6EE",
    "titleBar.activeForeground": "#0E0C12",
    "titleBar.inactiveBackground": "#E8CFDA",
    "titleBar.inactiveForeground": "#6E5E82",

    "panel.background": "#F8E6EE",
    "panel.border": "#A78DBF",
    "panelTitle.activeForeground": "#C06A78",
    "panelTitle.inactiveForeground": "#6E5E82",

    "tab.activeBackground": "#F8E6EE",
    "tab.activeForeground": "#0E0C12",
    "tab.inactiveBackground": "#E8CFDA",
    "tab.inactiveForeground": "#1E1A26",
    "tab.border": "#A78DBF",

    "input.background": "#FFFFFF",
    "input.foreground": "#0E0C12",
    "input.border": "#A78DBF",

    "list.activeSelectionBackground": "#E6A3B4",
    "list.activeSelectionForeground": "#0E0C12",
    "list.inactiveSelectionBackground": "#E6A3B4",
    "list.inactiveSelectionForeground": "#0E0C12",
    "list.hoverBackground": "#F2D2DF",
    "list.focusBackground": "#E6A3B4"
  },

  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "support.type.property-name.json",
          "entity.name.tag.json"
        ],
        "settings": {
          "foreground": "#0E0C12"
        }
      },
      {
        "scope": [
          "string.json"
        ],
        "settings": {
          "foreground": "#B76BB3"
        }
      },
      {
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "foreground": "#2E8F59"
        }
      },
      {
        "scope": [
          "string",
          "string.quoted",
          "string.template"
        ],
        "settings": {
          "foreground": "#B76BB3"
        }
      },
      {
        "scope": [
          "keyword",
          "storage",
          "storage.type"
        ],
        "settings": {
          "foreground": "#C06A78"
        }
      },
      {
        "scope": [
          "entity.name.type",
          "support.type",
          "entity.name.class",
          "support.class"
        ],
        "settings": {
          "foreground": "#9C5BC6"
        }
      },
      {
        "scope": [
          "entity.name.function",
          "support.function"
        ],
        "settings": {
          "foreground": "#B76A88"
        }
      },
      {
        "scope": [
          "variable",
          "variable.other",
          "variable.parameter",
          "variable.css",
          "support.type.custom-property.css",
          "meta.property-name.css",
          "variable.argument.css",
          "meta.function.variable.css",
          "meta.property-value.css",
          "meta.property-list.css",
          "source.css"
        ],
        "settings": {
          "foreground": "#6B78E6"
        }
      },
      {
        "scope": [
          "constant",
          "constant.language",
          "constant.numeric"
        ],
        "settings": {
          "foreground": "#B76BB3"
        }
      },
      {
        "scope": [
          "punctuation",
          "meta.brace"
        ],
        "settings": {
          "foreground": "#3E384C"
        }
      },
      {
        "scope": [
            "meta.selector.css",
            "entity.other.attribute-name.css",
            "support.type.property-name.css",
            "keyword.control.pseudo-class.css"
        ],
        "settings": {
            "foreground": "#6B78E6"   
        }
        }
    ]
  },

  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "property": "#0E0C12",
      "propertyKey": "#0E0C12",

      "type": "#9C5BC6",
      "class": "#9C5BC6",
      "interface": "#9C5BC6",
      "enum": "#9C5BC6",

      "function": "#B76A88",
      "method": "#B76A88",

      "variable": "#6B78E6",
      "parameter": "#3E384C",
      "property.readonly": "#0E0C12",

      "keyword": "#C06A78",
      "string": "#B76BB3",
      "number": "#B76BB3"
    }
  }
}